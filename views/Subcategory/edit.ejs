<html>
<!-- Compiled and minified CSS -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
<!--Import Google Icon Font-->
<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
<!--Import materialize.css-->
<link type="text/css" rel="stylesheet" href="css/materialize.min.css" media="screen,projection" />

<!-- Compiled and minified JavaScript -->
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

<style>
    .maincontainer {
        display: flex;
        justify-content: center;
        align-items: center;
        height: 100vh;
        width: 100vw;
        background-color: #2f3640;
    }

    .subcontainer {
        width: 700px;
        justify-content: center;
        align-items: center;
        padding: 10px;
        background-color: #dcdde1;
    }

    .heading {
        font-size: 30px;
        font-weight: 100px;
        text-align: center;
        color: #2f3640;

    }
</style>

<body style="margin:0px">
    <div class="maincontainer">
        <div class="subcontainer">
            <div class="heading">
                SubCategory Interface
            </div>
            <form action="/subcategory/edit" method="post">
                <div class="row">
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="SubCategory Id" name="subcategoryid" value="<%=data.subcategoryid%>" type="text"
                                class="validate" readonly>
                            <label for="SubCategory Id">SubCategory Id</label>
                        </div>
                        <div class="input-field col s12">
                            <input id="Category Id" name="categoryid" value="<%=data.categoryid%>" type="text"
                                class="validate">
                            <label for="Category Id">Category Id</label>
                        </div>

                        <div class="input-field col s12">
                            <input id="SubCategory Name" name="subcategoryname" value="<%=data.subcategoryname%>"
                                type="text" class="validate">
                            <label for="SubCategory Name">SubCategory Name</label>
                        </div>
                        <div class="input-field col s12">
                            <input id="Description" name="description" value="<%=data.description%>" type="text"
                                class="validate">
                            <label for="Description">Description</label>
                        </div>
                        <div class="row">
                            <label>
                                <div class="col s6 center">
                                    <input class="material-icons right" name="status" value="available" type="radio" <%=data.status=="available"?'checked':""%> />
                                    <span>Available</span>
                                </div>
                            </label>
                            <label>
                                <div class="col s6 center">
                                    <input class="material-icons right" name="status" value="not available"
                                        type="radio" <%=data.status=="not available"?'checked':""%> />
                                    <span> Not Available</span>
                                </div>
                            </label>
                        </div>

                    </div>
                </div>

                <div class="row">
                    <div class="col s6 center">
                        <button class="btn waves-effect waves-light" type="submit" name="action">Submit
                            <i class="material-icons right">send</i>
                        </button>

                    </div>


                    <div class=" col s6">
                        <button class="btn waves-effect waves-light" type="reset" name="action">Reset
                            <i class="material-icons right">cancel</i>
                        </button>

                    </div>


                </div>
            </form>
        </div>
    </div>
    </div>

    </div>






    </div>

    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">

    <!-- Compiled and minified JavaScript -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>

</body>

</html>